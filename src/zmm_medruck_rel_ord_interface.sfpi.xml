     <INPUT_PARAMETERS/>
     <OUTPUT_PARAMETERS/>
     <INITIALIZATION>
      <FPCLINE>TYPES: BEGIN OF ty_item_addr,</FPCLINE>
      <FPCLINE>         ebelp TYPE ebelp,</FPCLINE>
      <FPCLINE>         adrnr TYPE ad_addrnum,</FPCLINE>
      <FPCLINE>       END OF ty_item_addr.</FPCLINE>
      <FPCLINE>DATA: lt_addr    TYPE TABLE OF ty_item_addr,</FPCLINE>
      <FPCLINE>      ls_addr    TYPE ty_item_addr,</FPCLINE>
      <FPCLINE>      ls_usr_adr TYPE bapiaddr3,</FPCLINE>
      <FPCLINE>      lt_return  TYPE TABLE OF bapiret2,</FPCLINE>
      <FPCLINE>      ls_po_form_info TYPE zst_po_form_info.</FPCLINE>
      <FPCLINE>* Check the Segmentation switch</FPCLINE>
      <FPCLINE>CALL METHOD cl_ops_switch_check=&gt;sfsw_segmentation</FPCLINE>
      <FPCLINE>  RECEIVING</FPCLINE>
      <FPCLINE>    rv_active = gv_switch.</FPCLINE>
      <FPCLINE>IF gv_switch NE &apos;X&apos;.</FPCLINE>
      <FPCLINE>  CLEAR gv_switch.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE>*BREAK gpylla.</FPCLINE>
      <FPCLINE>DATA : gs_items TYPE zst_po_form_item.</FPCLINE>
      <FPCLINE>DATA lt_deladdrs TYPE ztt_po_form_dlvr_addrs.</FPCLINE>
      <FPCLINE>DATA ls_deladdrs TYPE zst_po_form_dlvr_addrs.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>*Get TVARVC values</FPCLINE>
      <FPCLINE>SELECT name,low FROM tvarvc INTO TABLE @DATA(lt_tvarvc)</FPCLINE>
      <FPCLINE>  WHERE name IN (&apos;ZMM_PO_BISMT_MAT_TYPES&apos;,&apos;ZMM_PO_MFRPN_MAT_TYPES&apos;).</FPCLINE>
      <FPCLINE/>
      <FPCLINE>gs_header-rel_order_no = header-ebeln.</FPCLINE>
      <FPCLINE>gs_header-po_number           = header-unsez.</FPCLINE>
      <FPCLINE>gs_header-plant               = header-reswk.</FPCLINE>
      <FPCLINE>SELECT SINGLE name2 FROM t001w INTO @DATA(lv_name2)</FPCLINE>
      <FPCLINE>  WHERE werks = @header-reswk.</FPCLINE>
      <FPCLINE>IF lv_name2 IS NOT INITIAL.</FPCLINE>
      <FPCLINE> gs_header-plant = lv_name2.</FPCLINE>
      <FPCLINE>ELSE.</FPCLINE>
      <FPCLINE> gs_header-plant = header-reswk.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>gs_header-created_by          = header-ernam.</FPCLINE>
      <FPCLINE>*CREATED_BY</FPCLINE>
      <FPCLINE>CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;</FPCLINE>
      <FPCLINE>  EXPORTING</FPCLINE>
      <FPCLINE>    username = header-ernam</FPCLINE>
      <FPCLINE>  IMPORTING</FPCLINE>
      <FPCLINE>    address  = ls_usr_adr</FPCLINE>
      <FPCLINE>  TABLES</FPCLINE>
      <FPCLINE>    return   = lt_return.</FPCLINE>
      <FPCLINE> IF ls_usr_adr-fullname IS NOT INITIAL.</FPCLINE>
      <FPCLINE>   gs_header-created_by = ls_usr_adr-fullname.</FPCLINE>
      <FPCLINE> ELSE.</FPCLINE>
      <FPCLINE>   gs_header-created_by = header-ernam.</FPCLINE>
      <FPCLINE> ENDIF.</FPCLINE>
      <FPCLINE>*TEL_NUMBER</FPCLINE>
      <FPCLINE>IF contactinfo-tel_number IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  gs_header-tel_number = |{ contactinfo-tel_number }| &amp;&amp; |    Ext: | &amp;&amp; |{ contactinfo-tel_extens }|.</FPCLINE>
      <FPCLINE>ELSE.</FPCLINE>
      <FPCLINE>  gs_header-tel_number = |{ contactinfo-ektel }| &amp;&amp; |    Ext: | &amp;&amp; |{ contactinfo-tel_extens }|.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE>gs_header-fax                 = contactinfo-telfx.</FPCLINE>
      <FPCLINE>gs_header-email_adr           = contactinfo-smtp_addr.</FPCLINE>
      <FPCLINE>gs_header-contact_person = contactinfo-eknam.</FPCLINE>
      <FPCLINE>gs_header-currency_code       = header-waers.</FPCLINE>
      <FPCLINE>gs_header-rel_order_no        = header-ebeln.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT  SINGLE eindt FROM eket</FPCLINE>
      <FPCLINE>    INTO @DATA(lv_eindt)</FPCLINE>
      <FPCLINE>    WHERE ebeln EQ @header-ebeln.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>CONCATENATE lv_eindt+4(2)     &apos;/&apos; lv_eindt+6(2)     &apos;/&apos; lv_eindt+0(4)     INTO gs_header-req_del_fright.</FPCLINE>
      <FPCLINE>CONCATENATE header-lfdat+4(2) &apos;/&apos; header-lfdat+6(2) &apos;/&apos; header-lfdat+0(4) INTO gs_header-req_delv_date.</FPCLINE>
      <FPCLINE>CONCATENATE header-aedat+4(2) &apos;/&apos; header-aedat+6(2) &apos;/&apos; header-aedat+0(4) INTO gs_header-po_date.</FPCLINE>
      <FPCLINE>CONCATENATE header-bedat+4(2) &apos;/&apos; header-bedat+6(2) &apos;/&apos; header-bedat+0(4) INTO gs_header-order_date.</FPCLINE>
      <FPCLINE>CONCATENATE header-bedat+4(2) &apos;/&apos; header-bedat+6(2) &apos;/&apos; header-bedat+0(4) INTO gs_header-order_date.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>DATA : lv_rcomp TYPE t880-rcomp.</FPCLINE>
      <FPCLINE>DATA : lv_qty(30) TYPE c.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>*lv_rcomp = |00| &amp; |{ header-bukrs }|.</FPCLINE>
      <FPCLINE>*</FPCLINE>
      <FPCLINE>*IF header-bukrs = &apos;1000&apos; OR header-bukrs = &apos;9950&apos;.</FPCLINE>
      <FPCLINE>*  lv_rcomp = &apos;007005&apos;.</FPCLINE>
      <FPCLINE>*ENDIF.</FPCLINE>
      <FPCLINE>*SELECT SINGLE * FROM t880 INTO gs_cc_addr WHERE rcomp = lv_rcomp .</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF header-bukrs = &apos;9950&apos;.</FPCLINE>
      <FPCLINE> DATA(lv_bukrs) = &apos;7005&apos;.</FPCLINE>
      <FPCLINE>ELSE.</FPCLINE>
      <FPCLINE> lv_bukrs = header-bukrs.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE adrnr FROM t001 INTO @DATA(lv_adrnr_1)</FPCLINE>
      <FPCLINE>       WHERE bukrs = @lv_bukrs.</FPCLINE>
      <FPCLINE>IF sy-subrc IS INITIAL.</FPCLINE>
      <FPCLINE>  SELECT SINGLE  addrnumber,name1,house_num1,mc_street,city1,region,post_code1</FPCLINE>
      <FPCLINE>    FROM adrc INTO @DATA(ls_adrc_1)</FPCLINE>
      <FPCLINE>    WHERE addrnumber = @lv_adrnr_1.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE>gs_cc_addr-name1 = ls_adrc_1-name1.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>*COMP_ADR_STREET</FPCLINE>
      <FPCLINE>CONCATENATE ls_adrc_1-house_num1 ls_adrc_1-mc_street INTO gs_cc_addr-stret</FPCLINE>
      <FPCLINE>   SEPARATED BY space.</FPCLINE>
      <FPCLINE>*gs_form_info-comp_adr_street = ls_t880-stret.</FPCLINE>
      <FPCLINE>*COMP_ADR_POST</FPCLINE>
      <FPCLINE>CONCATENATE ls_adrc_1-city1 ls_adrc_1-region ls_adrc_1-post_code1</FPCLINE>
      <FPCLINE>  INTO gs_cc_addr-postal_code SEPARATED BY space.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>DATA lv_adrnr TYPE adrnr.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE name1, stras, ort01, regio, pstlz, adrnr</FPCLINE>
      <FPCLINE>  FROM t001w INTO @gs_sup_plant_addr</FPCLINE>
      <FPCLINE>  WHERE werks = @header-reswk.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>lv_adrnr = gs_sup_plant_addr-telfx.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>gs_sup_plant_addr-ort01_regio_pstlz = |{ gs_sup_plant_addr-ort01 } { space } { gs_sup_plant_addr-regio } { space }  { gs_sup_plant_addr-pstlz }|.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE fax_number, tel_number</FPCLINE>
      <FPCLINE>       FROM adrc INTO ( @gs_sup_plant_addr-telfx, @gs_sup_plant_addr-telf1 )</FPCLINE>
      <FPCLINE>       WHERE addrnumber = @lv_adrnr.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT SINGLE smtp_addr FROM adr6 INTO @gs_sup_plant_addr-smtp_addr</FPCLINE>
      <FPCLINE>           WHERE addrnumber = @lv_adrnr.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>SELECT ebeln,</FPCLINE>
      <FPCLINE>       ebelp,</FPCLINE>
      <FPCLINE>       werks,</FPCLINE>
      <FPCLINE>       lgort,</FPCLINE>
      <FPCLINE>       menge,</FPCLINE>
      <FPCLINE>       meins,</FPCLINE>
      <FPCLINE>       adrnr,</FPCLINE>
      <FPCLINE>       adrn2 FROM ekpo INTO TABLE @DATA(lt_ekpo)</FPCLINE>
      <FPCLINE>  WHERE ebeln = @header-ebeln.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF sy-subrc IS INITIAL.</FPCLINE>
      <FPCLINE>*  DATA(lt_ekpo_tmp) = lt_ekpo.</FPCLINE>
      <FPCLINE>*  DELETE lt_ekpo_tmp WHERE lgort IS INITIAL.</FPCLINE>
      <FPCLINE>*  IF lt_ekpo_tmp IS NOT INITIAL.</FPCLINE>
      <FPCLINE>*    SELECT * FROM twlad INTO TABLE @DATA(lt_twlad)</FPCLINE>
      <FPCLINE>*      FOR ALL ENTRIES IN @lt_ekpo</FPCLINE>
      <FPCLINE>*      WHERE werks = @lt_ekpo-werks</FPCLINE>
      <FPCLINE>*      AND   lgort = @lt_ekpo-lgort.</FPCLINE>
      <FPCLINE>*  ENDIF.</FPCLINE>
      <FPCLINE>*  lt_ekpo_tmp = lt_ekpo.</FPCLINE>
      <FPCLINE>*  DELETE lt_ekpo_tmp WHERE werks IS INITIAL.</FPCLINE>
      <FPCLINE>*  IF lt_ekpo_tmp IS NOT INITIAL.</FPCLINE>
      <FPCLINE>*    SELECT werks, adrnr FROM t001w INTO TABLE @DATA(lt_t001w)</FPCLINE>
      <FPCLINE>*      FOR ALL ENTRIES IN @lt_ekpo</FPCLINE>
      <FPCLINE>*      WHERE werks = @lt_ekpo-werks.</FPCLINE>
      <FPCLINE>*  ENDIF.</FPCLINE>
      <FPCLINE>*</FPCLINE>
      <FPCLINE>*  SELECT addrnumber,</FPCLINE>
      <FPCLINE>*         name1,</FPCLINE>
      <FPCLINE>*         house_num1,</FPCLINE>
      <FPCLINE>*         mc_street,</FPCLINE>
      <FPCLINE>*         city1,</FPCLINE>
      <FPCLINE>*         region,</FPCLINE>
      <FPCLINE>*         post_code1,</FPCLINE>
      <FPCLINE>*         fax_number,</FPCLINE>
      <FPCLINE>*         tel_number FROM adrc</FPCLINE>
      <FPCLINE>*         INTO TABLE @DATA(lt_adrc)</FPCLINE>
      <FPCLINE>*         FOR ALL ENTRIES IN @lt_ekpo</FPCLINE>
      <FPCLINE>*         WHERE addrnumber = @lt_ekpo-adrn2.</FPCLINE>
      <FPCLINE>*</FPCLINE>
      <FPCLINE>*  SELECT addrnumber,</FPCLINE>
      <FPCLINE>*         name1,</FPCLINE>
      <FPCLINE>*         house_num1,</FPCLINE>
      <FPCLINE>*         mc_street,</FPCLINE>
      <FPCLINE>*         city1,</FPCLINE>
      <FPCLINE>*         region,</FPCLINE>
      <FPCLINE>*         post_code1,</FPCLINE>
      <FPCLINE>*         fax_number,</FPCLINE>
      <FPCLINE>*         tel_number FROM adrc</FPCLINE>
      <FPCLINE>*         APPENDING TABLE @lt_adrc</FPCLINE>
      <FPCLINE>*         FOR ALL ENTRIES IN @lt_ekpo</FPCLINE>
      <FPCLINE>*         WHERE addrnumber = @lt_ekpo-adrnr.</FPCLINE>
      <FPCLINE>*  IF lt_twlad IS NOT INITIAL.</FPCLINE>
      <FPCLINE>*    SELECT addrnumber,</FPCLINE>
      <FPCLINE>*           name1,</FPCLINE>
      <FPCLINE>*           house_num1,</FPCLINE>
      <FPCLINE>*           mc_street,</FPCLINE>
      <FPCLINE>*           city1,</FPCLINE>
      <FPCLINE>*           region,</FPCLINE>
      <FPCLINE>*           post_code1,</FPCLINE>
      <FPCLINE>*           fax_number,</FPCLINE>
      <FPCLINE>*           tel_number FROM adrc</FPCLINE>
      <FPCLINE>*           APPENDING TABLE @lt_adrc</FPCLINE>
      <FPCLINE>*           FOR ALL ENTRIES IN @lt_twlad</FPCLINE>
      <FPCLINE>*           WHERE addrnumber = @lt_twlad-adrnr.</FPCLINE>
      <FPCLINE>*  ENDIF.</FPCLINE>
      <FPCLINE>*  IF lt_t001w IS NOT INITIAL.</FPCLINE>
      <FPCLINE>*    SELECT addrnumber,</FPCLINE>
      <FPCLINE>*           name1,</FPCLINE>
      <FPCLINE>*           house_num1,</FPCLINE>
      <FPCLINE>*           mc_street,</FPCLINE>
      <FPCLINE>*           city1,</FPCLINE>
      <FPCLINE>*           region,</FPCLINE>
      <FPCLINE>*           post_code1,</FPCLINE>
      <FPCLINE>*           fax_number,</FPCLINE>
      <FPCLINE>*           tel_number FROM adrc</FPCLINE>
      <FPCLINE>*           APPENDING TABLE @lt_adrc</FPCLINE>
      <FPCLINE>*           FOR ALL ENTRIES IN @lt_t001w</FPCLINE>
      <FPCLINE>*           WHERE addrnumber = @lt_t001w-adrnr.</FPCLINE>
      <FPCLINE>*  ENDIF.</FPCLINE>
      <FPCLINE>*</FPCLINE>
      <FPCLINE>*  IF sy-subrc IS INITIAL.</FPCLINE>
      <FPCLINE>*    SELECT addrnumber,</FPCLINE>
      <FPCLINE>*           smtp_addr FROM adr6</FPCLINE>
      <FPCLINE>*           INTO TABLE @DATA(lt_adr6)</FPCLINE>
      <FPCLINE>*           FOR ALL ENTRIES IN @lt_adrc</FPCLINE>
      <FPCLINE>*           WHERE addrnumber = @lt_adrc-addrnumber.</FPCLINE>
      <FPCLINE>*  ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE>*BREAK gpylla.</FPCLINE>
      <FPCLINE>*IF item[] IS NOT INITIAL.</FPCLINE>
      <FPCLINE>*  SELECT matnr,</FPCLINE>
      <FPCLINE>*         bismt FROM mara</FPCLINE>
      <FPCLINE>*         INTO TABLE @DATA(lt_mara)</FPCLINE>
      <FPCLINE>*         FOR ALL ENTRIES IN @item</FPCLINE>
      <FPCLINE>*         WHERE matnr = @item-ematn.</FPCLINE>
      <FPCLINE>*ENDIF.</FPCLINE>
      <FPCLINE>IF item[] IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT  matnr,mtart,bismt,mfrpn FROM mara INTO TABLE @DATA(lt_mara)</FPCLINE>
      <FPCLINE>   FOR ALL ENTRIES IN @item</FPCLINE>
      <FPCLINE> WHERE matnr = @item-matnr.</FPCLINE>
      <FPCLINE>  IF sy-subrc IS INITIAL.</FPCLINE>
      <FPCLINE>    SORT lt_mara BY matnr.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE/>
      <FPCLINE>LOOP AT item[] INTO DATA(ls_item).</FPCLINE>
      <FPCLINE>  CLEAR ls_addr.</FPCLINE>
      <FPCLINE>  IF ls_item-adrnr IS NOT INITIAL.</FPCLINE>
      <FPCLINE>    ls_addr-adrnr = ls_item-adrnr.</FPCLINE>
      <FPCLINE>  ELSEIF ls_item-adrn2 IS NOT INITIAL.</FPCLINE>
      <FPCLINE>    ls_addr-adrnr = ls_item-adrn2.</FPCLINE>
      <FPCLINE>  ELSE.</FPCLINE>
      <FPCLINE>    IF ls_item-lgort IS NOT INITIAL</FPCLINE>
      <FPCLINE>     AND ls_item-werks IS NOT INITIAL.</FPCLINE>
      <FPCLINE>      SELECT SINGLE adrnr FROM twlad INTO ls_addr-adrnr</FPCLINE>
      <FPCLINE>         WHERE werks  EQ  ls_item-werks</FPCLINE>
      <FPCLINE>          AND lgort EQ ls_item-lgort.</FPCLINE>
      <FPCLINE>      IF sy-subrc IS NOT INITIAL.</FPCLINE>
      <FPCLINE>        CLEAR ls_addr-adrnr.</FPCLINE>
      <FPCLINE>      ENDIF.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE>    IF ls_addr-adrnr IS INITIAL AND ls_item-werks IS NOT INITIAL.</FPCLINE>
      <FPCLINE>      SELECT SINGLE adrnr</FPCLINE>
      <FPCLINE>        FROM t001w INTO ls_addr-adrnr</FPCLINE>
      <FPCLINE>        WHERE werks = ls_item-werks.</FPCLINE>
      <FPCLINE>      IF sy-subrc IS NOT INITIAL.</FPCLINE>
      <FPCLINE>        CLEAR ls_addr-adrnr.</FPCLINE>
      <FPCLINE>      ENDIF.</FPCLINE>
      <FPCLINE>    ENDIF.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>  IF ls_addr IS NOT INITIAL.</FPCLINE>
      <FPCLINE>    ls_addr-ebelp = ls_item-ebelp.</FPCLINE>
      <FPCLINE>    APPEND ls_addr TO lt_addr.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>ENDLOOP.</FPCLINE>
      <FPCLINE>DATA(lt_addr_tmp) = lt_addr.</FPCLINE>
      <FPCLINE>SORT lt_addr_tmp BY adrnr.</FPCLINE>
      <FPCLINE>DELETE ADJACENT DUPLICATES FROM lt_addr_tmp COMPARING adrnr.</FPCLINE>
      <FPCLINE>DATA(lv_addr_count) = lines( lt_addr_tmp ).</FPCLINE>
      <FPCLINE/>
      <FPCLINE>IF lt_addr_tmp[] IS NOT INITIAL.</FPCLINE>
      <FPCLINE>  SELECT  addrnumber,name1,house_num1,mc_street,city1,region,post_code1,tel_number,fax_number</FPCLINE>
      <FPCLINE>    FROM adrc INTO TABLE @DATA(lt_adrc)</FPCLINE>
      <FPCLINE>    FOR ALL ENTRIES IN @lt_addr_tmp</FPCLINE>
      <FPCLINE>    WHERE addrnumber = @lt_addr_tmp-adrnr.</FPCLINE>
      <FPCLINE>  IF sy-subrc IS INITIAL.</FPCLINE>
      <FPCLINE>    SELECT  addrnumber,smtp_addr &quot;#EC CI_NO_TRANSFORM</FPCLINE>
      <FPCLINE>      FROM adr6 INTO TABLE @DATA(lt_adr6)</FPCLINE>
      <FPCLINE>      FOR ALL ENTRIES IN @lt_adrc</FPCLINE>
      <FPCLINE>      WHERE addrnumber = @lt_adrc-addrnumber.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>ENDIF.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>*&gt;&gt; Get Delivery address details</FPCLINE>
      <FPCLINE>*// For single delivery address</FPCLINE>
      <FPCLINE>IF lv_addr_count = 1.</FPCLINE>
      <FPCLINE>  ls_addr =  lt_addr_tmp[ 1 ].</FPCLINE>
      <FPCLINE>  READ TABLE lt_adrc INTO DATA(ls_adrc)</FPCLINE>
      <FPCLINE>                     WITH KEY addrnumber = ls_addr-adrnr.</FPCLINE>
      <FPCLINE>  IF sy-subrc IS INITIAL.</FPCLINE>
      <FPCLINE>*  DELV_ADR_NAME</FPCLINE>
      <FPCLINE>    ls_po_form_info-delv_adr_name = ls_adrc-name1.</FPCLINE>
      <FPCLINE>*  DELV_ADR_STREET</FPCLINE>
      <FPCLINE>    CONCATENATE ls_adrc-house_num1 ls_adrc-mc_street INTO ls_po_form_info-delv_adr_street</FPCLINE>
      <FPCLINE>       SEPARATED BY space.</FPCLINE>
      <FPCLINE>*  DELV_ADR_CITY_REGION_POST</FPCLINE>
      <FPCLINE>    CONCATENATE ls_adrc-city1 ls_adrc-region ls_adrc-post_code1</FPCLINE>
      <FPCLINE>     INTO ls_po_form_info-delv_adr_city_region_post</FPCLINE>
      <FPCLINE>     SEPARATED BY space.</FPCLINE>
      <FPCLINE>*  DELV_ADR_FAX</FPCLINE>
      <FPCLINE>    ls_po_form_info-delv_adr_fax = ls_adrc-fax_number.</FPCLINE>
      <FPCLINE>*  DELV_ADR_PHONE</FPCLINE>
      <FPCLINE>    ls_po_form_info-delv_adr_phone = ls_adrc-tel_number.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>  READ TABLE lt_adr6 INTO DATA(ls_adr6)</FPCLINE>
      <FPCLINE>                     WITH KEY addrnumber = ls_addr-adrnr.</FPCLINE>
      <FPCLINE>  IF sy-subrc IS INITIAL.</FPCLINE>
