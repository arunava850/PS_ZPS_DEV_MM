      <FPCLINE/>
      <FPCLINE/>
      <FPCLINE>IF gs_items-material IS NOT INITIAL.</FPCLINE>
      <FPCLINE>*ORD_QTY</FPCLINE>
      <FPCLINE>  IF ls_item-umren &gt; 0.</FPCLINE>
