      <FPCLINE>  ELSE.</FPCLINE>
      <FPCLINE>*   &lt;lfs_item&gt;-ord_qty = ls_item-menge.</FPCLINE>
      <FPCLINE>  ENDIF.</FPCLINE>
      <FPCLINE>*UOM</FPCLINE>
      <FPCLINE>  gs_items-uom = ls_item-lmein.</FPCLINE>
      <FPCLINE>ELSE.</FPCLINE>
      <FPCLINE> gs_items-ord_qty = ls_item-menge.</FPCLINE>
      <FPCLINE>  gs_items-uom = ls_item-meins.</FPCLINE>
      <FPCLINE> gs_items-sku = ls_item-idnlf. &quot; 22/02/2024</FPCLINE>
